	<queryString>
		<![CDATA[SELECT
     p.nama AS 'nama',
     SUM(i.harga *  i.jumlah) AS 'total'
FROM
     workorder wo INNER JOIN itemworkorder i ON wo.id = i.workOrder_id
     INNER JOIN work w ON i.work_id = w.id
     INNER JOIN itempakaian p ON w.id=p.id
GROUP BY
	p.nama
ORDER BY
	p.nama;]]>
	</queryString>
